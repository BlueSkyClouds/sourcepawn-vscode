            },
            "2": {
              "name": "meta.preprocessor.macro.sourcepawn"
            }
          }
        },
        {
          "comment": "Preprocessor #if defined",
          "match": "(#if)\\s+(defined)\\s+(\\w+)",
          "captures": {
            "1": {
              "name": "keyword.control.misc.sourcepawn"
            },
            "2": {
              "name": "meta.preprocessor.conditional.sourcepawn"
