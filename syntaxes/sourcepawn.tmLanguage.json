            "2": { "name": "support.function.single-line.spdoc" },
            "3": { "name": "comment.sourcepawn" }
          }
        },
        {
          "match": "\\/\\/.*",
          "name": "comment.sourcepawn"
        },
        {
          "begin": "/\\*",
          "captures": {
            "0": {
              "name": "comment.sourcepawn"
            }
          },
          "end": "\\*/",
          "name": "comment.block.sourcepawn"
        }
      ]
    }
  }
